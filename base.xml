        </imagetype>
        <textarea name="text">
            <area>13,13,750,25</area>
            <font>basesmallblack</font>
        </textarea>
    </textedit>
    
    <!-- Base definition of a progressbar -->
    <progressbar name="baseprogressbar">
        <area>0,0,536,38</area>
        <layout>horizontal</layout>
        <style>reveal</style>
        <imagetype name="background">
            <area>0,0,100%,100%</area>
            <filename>progressbar_background.png</filename>
        </imagetype>
        <imagetype name="progressimage">
            <area>0,0,100%,100%</area>
            <filename>progressbar_fill.png</filename>
        </imagetype>
    </progressbar>
    
    <!-- Popup Dialogue Box -->
    <window name="MythPopupBox">
        <area>-1,-1,1280,720</area>
        <imagetype name="backimg">
            <filename>LCARSMenusbg.png</filename>
        </imagetype>
        
        <textarea name="messagearea" from="basetextarea">
            <area>200,40,1026,177</area>
            <font>baseheader</font>
            <multiline>yes</multiline>
            <align>allcenter</align>
        </textarea>
        
        <buttonlist name="list" from="basebuttonlist">
            <area>208,250,1002,414</area>
            <drawfrombottom>yes</drawfrombottom>
            <showarrow>no</showarrow>
            <wrapstyle>selection</wrapstyle>
            <statetype name="buttonitem">
                <state name="active">
                    <textarea name="buttontext">
                        <area>0,0,100%,30</area>
                        <align>allcenter</align>
                    </textarea>
                </state>
                <state name="selectedinactive">
                    <textarea name="buttontext">
                        <area>0,0,100%,30</area>
                        <align>allcenter</align>
                    </textarea>
                </state>
                <state name="selectedactive">
                    <textarea name="buttontext">
                        <area>0,0,100%,30</area>
                        <align>allcenter</align>
                    </textarea>
                </state>
            </statetype>
        </buttonlist>
    </window>
    
    <!-- Fullscreen Dialogue Box -->
    <window name="MythDialogBox">
        <area>-1,-1,1280,720</area>
        <imagetype name="backimg">
            <filename>LCARSMenusbg.png</filename>
        </imagetype>
        
        <textarea name="messagearea" from="basetextarea">
            <area>208,15,1002,169</area>
            <font>baselarge</font>
            <multiline>yes</multiline>
            <align>allcenter</align>
        </textarea>
        
        <buttonlist name="list" from="basebuttonlist">
            <area>208,250,1002,414</area>
            <drawfrombottom>yes</drawfrombottom>
            <showarrow>no</showarrow>
